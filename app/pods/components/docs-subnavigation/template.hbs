<ul class="pager">
  {{#if prevRoute}}
    <li>
      {{#link-to prevRouteName}}
        <b>Previous: </b> {{titleize-dashes prevRoute}}
      {{/link-to}}
    </li>
  {{/if}}
  {{#if nextRoute}}
    <li>
      {{#link-to nextRouteName}}
        <b>Next:</b> {{titleize-dashes nextRoute}}
      {{/link-to}}
    </li>
  {{/if}}
</ul>
